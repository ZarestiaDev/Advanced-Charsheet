<?xml version="1.0" encoding="iso-8859-1"?>

<root>
	<windowclass name="charsheet_inventory" merge="join">
		<sheetdata>
			<label name="slot_label" insertbefore="location_label">
				<anchored to="rightanchor" width="70">
					<top />
					<right anchor="left" relation="relative" offset="-10" />
				</anchored>
				<center/>
				<static textres="char_label_itemslot" />
			</label>
		</sheetdata>
	</windowclass>
	
	<windowclass name="char_invitem" merge="join">
		<sheetdata>
			<stringu name="slot" insertbefore="location">
				<anchored width="70" height="20">
					<top offset="2" />
					<right parent="rightanchor" anchor="left" relation="relative" offset="-10" />
				</anchored>
			</stringu>
		</sheetdata>
	</windowclass>
</root>
