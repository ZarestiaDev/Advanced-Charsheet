<?xml version="1.0" encoding="iso-8859-1"?>

<root>
	<windowclass name="charsheet_inventory" merge="join">
		<sheetdata>
			<label_list_content_string_noframe_right name="label_location">
				<anchored>
					<right offset="-20" />
				</anchored>
			</label_list_content_string_noframe_right>
			<label_list_content_string_noframe_right name="slot_label" insertbefore="label_location">
				<anchored>
					<right offset="20" />
				</anchored>
				<static textres="char_label_itemslot" />
			</label_list_content_string_noframe_right>
		</sheetdata>
	</windowclass>
	
	<windowclass name="char_invitem" merge="join">
		<sheetdata>
			<string_listitem_right name="slot" insertbefore="location">
				<anchored width="70" />
			</string_listitem_right>
		</sheetdata>
	</windowclass>
</root>
